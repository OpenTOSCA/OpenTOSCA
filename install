#!/bin/sh
if [ -z ${CONTAINER_VERSION}${TAG} ]; then
  export CONTAINER_VERSION=master
  export UI_VERSION=master
  export WINERY_VERSION=master

  export TAG="v2.0.0"
  printf "Using stable version TAG\n"
  printf "Override by using the environment variable TAG\n"
fi

export SCRIPTPATH="."

chmod +x $SCRIPTPATH/includeInstallCommon.sh
$SCRIPTPATH/includeInstallCommon.sh
chmod +x $SCRIPTPATH/includeInstallCommonStart.sh
$SCRIPTPATH/includeInstallCommonStart.sh